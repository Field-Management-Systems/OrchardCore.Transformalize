<cfg name="Migrations" read-only="true">
   <connections>
      <add name="input" provider="sqlite" file="..\App_Data\Sites\Default\yessql.db" />
      <add name="output" provider="file" delimiter="" file="migrations.json" header="" />
   </connections>
   <entities>
      <add name="Document">
         <filter>
            <add field="Type" value="OrchardCore.Data.Migration.Records.DataMigrationRecord, OrchardCore.Data" />
         </filter>
         <fields>
            <add name="Type" output="false" />
            <add name="Content" length="max" output="false">
               <transforms>
                  <add method="fromjson">
                     <fields>
                        <add name="DataMigrations" length="max" />
                     </fields>
                  </add>
               </transforms>
            </add>
         </fields>
      </add>
   </entities>
</cfg>