[{"Id":7,"Published":true,"ModifiedUtc":"2020-04-27 11 AM","DisplayText":"Bogus","Xml":"<cfg name=\"Bogus\">\r\n\r\n    <connections>\r\n        <add name=\"input\" provider=\"bogus\" seed=\"1\" />\r\n    </connections>\r\n\r\n    <entities>\r\n        <add name=\"Contact\" page=\"1\" size=\"20\">\r\n            <fields>\r\n                <add name=\"Identity\" type=\"int\" />\r\n                <add name=\"FirstName\" />\r\n                <add name=\"LastName\" />\r\n                <add name=\"Email\" />\r\n                <add name=\"Phone\" format=\"(###) ###-####\" />\r\n                <add name=\"City\" />\r\n                <add name=\"State\" length=\"2\" />\r\n                <add name=\"Latitude\" type=\"double\" />\r\n                <add name=\"Longitude\" type=\"double\" />\r\n            </fields>\r\n        </add>\r\n    </entities>\r\n\r\n</cfg>\r\n","View":"<a href=\"/report/bogus\" target=\"_blank\">View</a>"},{"Id":79,"Published":true,"ModifiedUtc":"2020-04-27 1 PM","DisplayText":"Reports","Xml":"<cfg name=\"Reports\">\r\n  \r\n  <connections>\r\n      <add name=\"input\" provider=\"sqlite\" file=\"..\\Site\\App_Data\\Sites\\Default\\yessql.db\" />\r\n  </connections>\r\n  \r\n  <entities>\r\n      <add name=\"Reports\" page=\"1\">\r\n        <fields>\r\n          <add name=\"Id\" type=\"long\" />\r\n          <add name=\"Published\" type=\"bool\" />\r\n          <add name=\"ModifiedUtc\" type=\"datetime\" label=\"Modified\" format=\"yyyy-MM-dd h tt\" \r\n               t=\"timezone(UTC,Eastern Standard Time)\" />\r\n          <add name=\"DisplayText\" label=\"Title\" />\r\n          <add name=\"Alias\" output=\"false\" />\r\n          <add name=\"Content\" length=\"max\" output=\"false\" />\r\n        </fields>\r\n        <calculated-fields>\r\n          <add name=\"Xml\" length=\"2048\"\r\n               t=\"copy(Content).jsonpath($.TransformalizeReportPart.Arrangement.Arrangement)\" />\r\n          <add name=\"View\" raw=\"true\" length=\"2048\"\r\n               t='format(<a href=\"/report/{Alias}\" target=\"_blank\">View</a>)' />  \r\n        </calculated-fields>\r\n    </add>\r\n  </entities>\r\n  \r\n  <actions>\r\n      <add type=\"log\" level=\"info\" message=\"Hello World\" /> \r\n      <add type=\"run\"\r\n           description=\"Create Reports View\"\r\n           before=\"true\" after=\"false\" \r\n           connection=\"input\"\r\n           command=\"\r\ncreate view if not exists Reports as\r\nselect \r\n\tc.Id\r\n\t,c.DocumentId\r\n\t,c.Published\r\n\t,c.ModifiedUtc\r\n\t,c.Author\r\n\t,c.DisplayText\r\n\t,a.Alias\r\n\t,d.Content\r\nfrom ContentItemIndex c\r\ninner join Document d on (c.DocumentId = d.Id)\r\ninner join AliasPartIndex a on (a.DocumentId = c.DocumentId)\r\nwhere c.ContentType = 'TransformalizeReport'\r\nand c.Latest = 1;\" />\r\n  </actions> \r\n  \r\n</cfg>\r\n","View":"<a href=\"/report/reports\" target=\"_blank\">View</a>"}]